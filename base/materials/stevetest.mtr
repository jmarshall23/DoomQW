models/mapobjects/steve_temp/a_tecpipebraid2soylent
{
	qer_editorimage		models/mapobjects/steve_temp/a_tecpipebraid2soylent.tga
        noSelfShadow
	{
		blend 		bumpmap
		map	        addnormals (textures/hell/hellbubbles_local.tga, heightmap ( textures/hell/slime_bblood01.tga, 6))
	}
	{
		blend 		diffusemap
		map		textures/hell/slime_dblood03.tga
		vertexColor
	}
	{
		blend		specularmap
		map	      textures/hell/slime_sblood_wall01.tga
		vertexColor
	}
	{
		blend		bumpmap
		map	addnormals (textures/object/a_tecpipebraid1_local.tga, heightmap (textures/object/a_tecpipebraid1_bmp.tga, 4))
                scale    2, 1
	}
	{
		blend		diffusemap
		map		textures/object/a_tecpipebraid2_d.tga
                scale    2, 1
		inverseVertexColor
	}
	{
		blend 		specularmap
		map		textures/object/a_tecpipebraid1_s.tga
                scale    2, 1
		inverseVertexColor
	}
}

