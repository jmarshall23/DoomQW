textures/hell/spectrum2_symbol_letters_02
{
	qer_editorimage textures/decals/symbol_letters_32d.tga
	DECAL_MACRO
	noShadows
	nonsolid
	noimpact
	twoSided
	translucent
	spectrum 2
	{       
      	blend	diffusemap
		map		textures/decals/symbol_letters_32d.tga
		red		(fireballtable[ time * .2]) * parm0
		green	(fireballtable[ time * .2]) * parm1
		blue	(fireballtable[ time * .2]) * parm2
	}
	{       
      	blend	specularmap
		map		textures/decals/symbol_letters_32d.tga
	}
}

