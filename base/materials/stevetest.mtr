textures/hell/bloodsharprock2blend
{
        noSelfShadow
	qer_editorimage		textures/hell/bloodsharprock2blend.tga
	stone

	{
		blend 		bumpmap
		map		addnormals( textures/hell/sharprock2_local.tga, heightmap( textures/hell/sharprock2_h.tga, 8 ) )
	}
	{
		blend 		diffusemap
		map		textures/hell/sharprock2.tga
		vertexColor
	}
	{
		blend		specularmap
		map	      textures/hell/sharprock2_s.tga
		vertexColor
	}
    {
		blend		bumpmap
		map	heightmap(textures/hell/blood_stream1_h.tga, 8 )
		translate	time * -.01 , time * -0.01
	}
		{
		blend		diffusemap
		map		textures/hell/blood_stream1.tga   
		translate	time * .015 , time * -.03
		inverseVertexColor
	}
	{
		blend 		specularmap
		map		textures/hell/blood_stream1_s.tga
		translate	time * .02 , time * -0.05
		inverseVertexColor

	}
} 
